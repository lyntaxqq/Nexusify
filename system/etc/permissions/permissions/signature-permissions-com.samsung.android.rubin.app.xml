        <permission name="com.sec.spp.permission.SEND"/>
        <permission name="android.permission.GET_INTENT_SENDER_INTENT"/>
        <permission name="com.samsung.permission.WRITE_SM_DATA"/>

    </signature-permissions>

</permissions>