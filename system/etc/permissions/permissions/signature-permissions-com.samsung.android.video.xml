<?xml version="1.0" encoding="utf-8"?>
<!--
  ~ This XML file declares which platform signature permissions to grant to
  ~ platform signed nonsystem apps.
  -->
 
<permissions>
    <signature-permissions package="com.samsung.android.video">
        <permission name="com.samsung.android.video.POPUP_PLAYER"/>
        <permission name="com.samsung.android.video.player.provider.SharedMemoryProvider.READ"/>
		<permission name="com.samsung.android.video.player.provider.SharedMemoryProvider.WRITE"/>
		<permission name="com.samsung.android.video.player.EXPORTED_ACTIVITY"/>
    </signature-permissions>
</permissions>